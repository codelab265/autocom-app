      "package": "com.autocom.codelab",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDoJl8RZOC0TUHMlaybKhNYtqJaiVt_ABo"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
